{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/login",
      "dest": "/api/login.py"
    },
    {
      "src": "/api/register",
      "dest": "/api/register.py"
    },
    {
      "src": "/api/favorites(.*)",
      "dest": "/api/favorites.py"
    },
    {
      "src": "/api/history(.*)",
      "dest": "/api/history.py"
    }
  ]
}

